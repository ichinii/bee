; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Task",
"class": "CollectNectarTask",
"language": "GDScript",
"path": "res://scripts/tasks/CollectNectar.gd"
}, {
"base": "Node2D",
"class": "PlayerStats",
"language": "GDScript",
"path": "res://scripts/PlayerStats.gd"
}, {
"base": "Node",
"class": "Task",
"language": "GDScript",
"path": "res://scripts/tasks/Task.gd"
}, {
"base": "Area2D",
"class": "Trigger",
"language": "GDScript",
"path": "res://scripts/Trigger.gd"
}, {
"base": "CanvasLayer",
"class": "UIController",
"language": "GDScript",
"path": "res://scripts/UIController.gd"
} ]
_global_script_class_icons={
"CollectNectarTask": "",
"PlayerStats": "",
"Task": "",
"Trigger": "",
"UIController": ""
}

[application]

config/name="bee"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://res/icon.png"

[autoload]

SingletonManager="*res://scripts/SingletonManager.gd"
IntersectionHelper="*res://scripts/Helper/IntersectionHelper.gd"
DeviceHelper="*res://scripts/Helper/DeviceHelper.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/resizable=false
window/size/test_width=540
window/size/test_height=960
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

environment/default_environment="res://default_env.tres"
